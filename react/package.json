    "eslint-plugin-react-refresh": "^0.4.12",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "globals": "^15.9.0",
    "identity-obj-proxy": "^3.0.0",
    "postcss": "^8.4.47",
    "postcss-apply": "^0.12.0",
    "postcss-custom-media": "^11.0.1",
